<h1>Listing names</h1>

<table>
  <thead>
    <tr>
      <th>Option</th>
      <th>Branch of medicine</th>
      <th>Review</th>
      <th>Doc preference</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name.option %></td>
        <td><%= name.branch_of_medicine %></td>
        <td><%= name.review %></td>
        <td><%= name.doc_preference %></td>
        <td><%= link_to 'Show', name %></td>
        <td><%= link_to 'Edit', edit_name_path(name) %></td>
        <td><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Name', new_name_path %>
